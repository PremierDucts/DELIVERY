<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             x:Class="DeliveryMobile.AppShell"
             Title="AppShell"
             FlyoutBehavior="Disabled"
             Shell.NavBarIsVisible="False"
             Shell.FlyoutItemIsVisible="False"
             xmlns:Views="clr-namespace:DeliveryMobile.Views"
             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             xct:SafeAreaEffect.SafeArea="True"
             BackgroundColor="{StaticResource PrimaryColor}"
             FlyoutBackgroundColor="{StaticResource ContentBackgroundColor}">
    <ShellContent Title="DeliveryPage" Route="EventCalendarDeliveryPage" ContentTemplate="{DataTemplate Views:EventCalendarDeliveryPage}"/>
</Shell>
